<h1> Listing all users </h1>

<table>
  <tr>
    <th>Username</th>
    <th>Password</th>
    <th>Email</th>
  </tr>
 
  <% @user.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.password %></td>
      <td><%= user.email %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
    </tr>
  <% end %>
</table>
